{
  "name": "test-flow",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.3",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start:js": "react-scripts start",
    "build:js": "react-scripts build",
    "start:eel": "pipenv run py test.py dev",
    "build:eel": "pipenv run py -m eel index.py build --onefile --noconsole --name test-flow --icon=public/favicon.ico --hiddenimport test_scripts",
    "clean:build": "rm -rf build dist",
    "start": "npm-run-all -p start:*",
    "build": "npm-run-all build:*",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
